    "def prod(k):\n",
    "    def méthode1(n):\n",
    "        l=[];\n",
    "        l.append(1);\n",
    "        s=0\n",
    "        som=2;\n",
    "        if s==0:\n",
    "            while som < int(k):\n",
    "                l.append(som)\n",
    "                i=som;j=som;t=1;\n",
    "                while i + j>int(k):\n",
    "                    j = l[-t];\n",
    "                    t += 1;\n",
    "                som = i+j;\n",
    "            n=len(l)\n",
    "            return n\n",
    "    def méthode2(n):\n",
    "        l=[]; \n",
    "        s=1;\n",
    "        l.append(1);\n",
    "        if s==1:\n",
    "            l.append(2);\n",
    "            som=3;\n",
    "            while som < int(k):\n",
    "                l.append(som)\n",
    "                i=som;j=som;t=1;\n",
    "                while i + j>int(k):\n",
    "                    j = l[-t];\n",
    "                    t += 1;\n",
    "                som = i+j;\n",
    "            m=len(l)\n",
    "            return m\n",
    "    m=méthode1(k);\n",
    "    n=méthode2(k);\n",
    "    if n>m:\n",
    "        nb_min=m\n",
    "    else:\n",
    "        nb_min=n\n",
    "    return nb_min\n",
    "\n",
    "k=input(\"donner la puissance choisi\")   \n",
    "print(\"le nbre min est \",prod(k))\n",
    "\n",
    "\n",
    "somme=0;\n",
    "for i in range(1,200):\n",
    "    somme += prod(i)\n",
    "print(\"resultat de la somme donnée est \",somme)\n",
    "\n",
    "\n"
